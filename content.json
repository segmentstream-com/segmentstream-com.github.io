---
---
{
  {% for entry in site.docs %}
    "{{ entry.url | slugify }}": {
      "title": "{{ entry.title | xml_escape }}",
      "url": "{{ entry.url | xml_escape }}",
      "summary": "{{ entry.content | strip_html | strip_newlines | xml_escape | slugify: 'ascii' | replace: '-quot', '' }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}
